// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Usuario {
  uid       Int
  nome      String
  email     String  @unique
  telefone  String
  cargo     String
  descricao String
  status    Boolean @default(true)
  tipo      Int     @default(1)
  grupo     Int
}
